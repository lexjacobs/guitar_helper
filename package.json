{
  "name": "guitar-helper",
  "version": "0.0.1",
  "description": "Guitar Scale Visualizer",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "start": "NODE_ENV=development node server",
    "build": "rm -rf ./dist/* && NODE_ENV=production webpack -p --config webpack.config.js",
    "nightwatch": "nightwatch ./test/nightwatchTest.js ./nightwatch.json",
    "test_watch": "mocha  --watch --compilers js:babel-core/register,js:./test/css-modules-compiler.js \"app/**/*@(.spec.js|.spec.jsx)\"",
    "test": "mocha --compilers js:babel-core/register,js:./test/css-modules-compiler.js \"app/**/*@(.spec.js|.spec.jsx)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lexjacobs/guitar_helper.git"
  },
  "keywords": [
    "guitar",
    "tablature",
    "scale",
    "practice"
  ],
  "author": "https://github.com/lexjacobs/",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/lexjacobs/guitar_helper/issues"
  },
  "homepage": "https://github.com/lexjacobs/guitar_helper#readme",
