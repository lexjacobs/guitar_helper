    "css-loader": "^0.26.1",
    "css-modules-require-hook": "^4.0.3",
    "dotenv": "^2.0.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-react": "^6.3.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.25.0",
    "jsdom": "^9.9.1",
    "jsdom-global": "^2.1.1",
    "mocha": "^3.0.2",
    "nightwatch": "^0.9.11",
    "node-sass": "^4.1.1",
